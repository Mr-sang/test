/* pages/member/index/index.wxss */
.main{
  padding: 19px 19px;
  background: linear-gradient(to right, #45A5FD , #6263FC);
  overflow: hidden;
}
page{
  background: #f5f5f5;

}
.main image{
  width: 80px;
  height: 80px;
  border-radius: 50%;
  margin-left:24rpx;
}
.main text{
  line-height: 80px;
  font-size: 15px;
  color: white;
  margin-left: 15px;
}
.li{
  margin-top: 10px;
  background: white;
  padding: 0 15px;
  overflow: hidden;
  height: 60px;
  line-height: 60px;
  font-size: 14px;
  color: #333333;
}
.li image.fl{
  width: 20px;
  margin-top: 20px;
  margin-right: 15px;
}
.li image.fr{
  width: 8px;
  margin-top: 22px;
}

.out-bottom{
  margin: 40rpx 30rpx 0;
}
.out-bottom button{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  width: 100%;
  background-color: #ff3b44;
  color: #fff;
  font-size: 30rpx;
  border-radius: 0;
}